{
  "$schema": "http://json-schema.org/draft-07/schema",
  "oneOf": [
    { "type": "null" },
    {
      "type": "object",
      "properties": {
        "index": { "type": "number" },
        "name": { "type": "string" },
        "category": { "type": "string" },
        "summaries": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "rank": { "type": "number" },
              "runner": {
                "type": "object",
                "properties": {
                  "pk": { "type": "string" },
                  "name": { "type": "string" },
                  "thumbnailUrl": { "type": "string" }
                },
                "additionalProperties": false,
                "required": ["pk", "name", "thumbnailUrl"]
              },
              "done": { "type": "number" },
              "score": { "type": "string" }
            },
            "additionalProperties": false,
            "required": ["rank", "runner", "done", "score"]
          }
        }
      },
      "additionalProperties": false,
      "required": ["index", "name", "category", "summaries"]
    }
  ]
}
