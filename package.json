{
  "name": "ajr-elimination-layouts",
  "version": "0.0.0",
  "description": "<description>",
  "homepage": "",
  "author": {
    "name": "Cma",
    "email": "chicchaima@gmail.com"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension"
  ],
  "keywords": [
    "node.js",
    "nodecg",
    "react",
    "nodecg-bundle"
  ],
  "scripts": {
    "autofix": "run-s autofix:*",
    "autofix:browser": "npm run lint:browser -- --fix",
    "autofix:nodecg": "npm run lint:nodecg -- --fix",
    "build": "webpack --mode=production",
    "lint": "run-s lint:*",
    "lint:browser": "eslint --ext .ts,.tsx src/browser",
    "lint:nodecg": "eslint --ext .ts src/nodecg",
    "dev": "webpack --watch --mode=development",
    "schema-types": "nodecg schema-types -o src/nodecg/generated schemas",
    "schema-types:lib": "nodecg schema-types -o src/nodecg/generated/lib schemas/lib",
    "test": "jest"
  },
  "nodecg": {
    "compatibleRange": ">=1.9.0",
    "dashboardPanels": [
      {
        "name": "switch-elimination",
        "title": "予選結果表示",
        "width": 4,
        "file": "switch-elimination.html"
      },
      {
        "name": "import-result",
        "title": "予選結果インポート",
        "width": 3,
        "file": "import-result.html",
        "workspace": "データ管理"
      }
    ],
    "graphics": [
      {
        "file": "general-view.html",
        "width": "1920",
        "height": "1080",
        "singleInstance": false
      },
      {
        "file": "result-view.html",
        "width": "1920",
        "height": "1080",
        "singleInstance": false
      }
    ]
  },
  "repository": "https://github.com/cma2819/ome-speedrun-layouts.git",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.3",
    "@mui/material": "^5.0.3",
    "clone": "^2.1.2",
    "discord.js": "^14.11.0",
    "globby": "^11.0.3",
    "googleapis": "^120.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-transition-group": "^4.4.5",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/clone": "^2.1.1",
    "@types/globby": "^9.1.0",
    "@types/html-webpack-plugin": "^3.2.5",
    "@types/jest": "^27.0.2",
    "@types/node": "^15.6.2",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-transition-group": "^4.4.3",
    "@types/styled-components": "^5.1.10",
    "@types/webpack-merge": "^5.0.0",
    "@types/webpack-node-externals": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "css-loader": "^6.3.0",
    "eslint": "^8.44.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.2.5",
    "nodecg-cli": "^8.0.1",
    "style-loader": "^3.3.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-nodecg": "^0.3.1",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
